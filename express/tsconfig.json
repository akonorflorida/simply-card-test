{
  "ts-node": {
     "swc": false,
     "transpileOnly": true
  },
  "compilerOptions": {
     "module": "commonjs",
     "forceConsistentCasingInFileNames": true,
     "target": "es2017",
     "lib": ["es6", "ES2021"],
     "allowJs": true,
     "outDir": "build",
     "rootDir": "src",
     "strict": true,
     "noImplicitAny": true,
     "esModuleInterop": true,
     "resolveJsonModule": true, 
     "downlevelIteration": true,
  }
}
